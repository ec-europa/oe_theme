{#
/**
 * @file
 * Default implementation for the social icon component.
 */
#}
{% if url %}
  {% include '@ecl/ec-component-social-icon' with {
    variant: service,
    link: {
      label: label,
      href: url
    }
  } only %}
{% else %}
  {% include '@ecl/ec-component-social-icon' with {
    variant: service,
    label: label
  } only %}
{% endif %}
