<?php

/**
 * @file
 * Functions to support theming.
 */

/**
 * Implements hook_preprocess_hook().
 */
function oe_theme_preprocess_page(&$variables) {

  // Allow third-party code to override logo display type.
  $variables['logo_type'] = isset($variables['logo_type']) ? $variables['logo_type'] : 'right';
}
