<?php

/**
 * @file
 * Functions to support theming.
 */

/**
 * Implements hook_preprocess_hook().
 */
function oe_theme_preprocess_breadcrumb(&$variables) {
  $variables['segments'] = [];
  foreach ($variables['breadcrumb'] as $item) {
    $variables['segments'][] = [
      'target' => $item['url'],
      'title' => $item['text'],
    ];
  }
}
